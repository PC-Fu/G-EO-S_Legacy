 <!-- Physical units -->
 <xsd:simpleType name="unittype">
  <xsd:restriction base="xsd:string">
   <xsd:pattern value="(-?\d*(\.\d*)?(e-?\d+)?(\s+\S+^?\S*/?\S*)?)?($:(\S+))?"/>
  </xsd:restriction>
 </xsd:simpleType>

 <!-- Array data -->
 <xsd:simpleType name="arraytype">
  <xsd:restriction base="xsd:string">
   <xsd:pattern value="\s*(-?\d*(\.\d*)?(e-?\d+)?\s*,\s*)*(-?\d*(\.\d*)?(e-?\d+)?)"/>
  </xsd:restriction>
 </xsd:simpleType>

 <!-- R1Tensor -->
 <xsd:simpleType name="tensortype">
  <xsd:restriction base="xsd:string">
   <xsd:pattern value="\s*(-?\d*(\.\d*)?(e-?\d+)?)\s+(-?\d*(\.\d*)?(e-?\d+)?)\s+(-?\d*(\.\d*)?(e-?\d+)?)"/>
  </xsd:restriction>
 </xsd:simpleType>
 
 <!-- Ranges -->
 <xsd:simpleType name="rangetype">
  <xsd:restriction base="xsd:string">
   <xsd:pattern value="(\d+(,|-|\s+)?)*\d+"/>
  </xsd:restriction>
 </xsd:simpleType>

 <xsd:simpleType name="srangetype">
  <xsd:restriction base="xsd:string">
   <xsd:pattern value="(\S+(,|-|\s+)?)*\S+"/>
  </xsd:restriction>
 </xsd:simpleType>
 
 <!-- Variable units: we may also want to add support for functions -->
 <xsd:complexType name="VariableValueType">
    <xsd:attribute name="constant" type="unittype"/>
    <xsd:attribute name="distribution" type="xsd:string"/>
 </xsd:complexType>

 <!-- DEFINITION FOR INCLUDES -->
  <xsd:complexType name="IncludeType">
   <xsd:attribute name="file" type="xsd:string" use="required"/>
 </xsd:complexType>

 <!-- IF statements -->
  <xsd:complexType name="ThenElseType"> 
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:any minOccurs="0"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="IFType">
    <xsd:sequence>
      <xsd:element name="Then" type="ThenElseType"
                  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="Else" type="ThenElseType"
                  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
    <xsd:attribute name="condition" type="xsd:string" use="required"/>
  </xsd:complexType>

  <xsd:group name="IfIncludeOption">
    <xsd:choice>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element name="If" type="IFType" />
        <xsd:element name="Include" type="IncludeType" />
      </xsd:choice>
    </xsd:choice>
  </xsd:group>
  <!-- To add if/include option: <xs:group ref="IfIncludeOption"/> -->
 
