   <xsd:element name="VectorField3D"   type="Table3DType" />
   <xsd:element name="VectorField4D"   type="Table4DType" />
   <xsd:element name="TableNUFT" type="TableNUFTType" />
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
  </xsd:choice>
 </xsd:complexType> 
 
 
  <!--FUNCTIONS-->
 <xsd:complexType name="SymbolicFunctionType">
   <xsd:attribute name="f" type="xsd:string"/>
   <xsd:attribute name="name" type="xsd:string"/>
   <xsd:attribute name="variables" type="xsd:string"/>
 </xsd:complexType>

 <xsd:complexType name="ConstantFunctionType">
   <xsd:attribute name="f" type="xsd:string"/>
   <xsd:attribute name="name" type="xsd:string"/>
 </xsd:complexType>

 <xsd:complexType name="PolynomialFunctionType">
   <xsd:attribute name="coeffs" type="xsd:string"/>
   <xsd:attribute name="name" type="xsd:string"/>
 </xsd:complexType>

 <xsd:complexType name="UniformRandomDistributionFunctionType">
   <xsd:attribute name="max" type="xsd:string"/>
   <xsd:attribute name="min" type="xsd:string"/>
   <xsd:attribute name="uniqueAcrossProcessors" type="xsd:string"/>
   <xsd:attribute name="name" type="xsd:string"/>
 </xsd:complexType>

 <xsd:complexType name="TableFunctionType">
   <xsd:attribute name="table" type="xsd:string"/>
   <xsd:attribute name="name" type="xsd:string"/>
 </xsd:complexType>

 <xsd:complexType name="FunctionsType">
   <xsd:choice minOccurs="0" maxOccurs="unbounded">
     <xsd:element name="SymbolicFunction"   type="SymbolicFunctionType"/>
     <xsd:element name="ConstantFunction"   type="ConstantFunctionType"/>
     <xsd:element name="PolynomialFunction"   type="PolynomialFunctionType"/>
     <xsd:element name="UniformRandomDistribution"   
                  type="UniformRandomDistributionFunctionType"/>
     <xsd:element name="Lookup4DTable"   type="TableFunctionType"/> <!-- Tables are automatically registered as functions of the same name -->
     <xsd:element name="Lookup3DTable"   type="TableFunctionType"/>
     <xsd:element name="Lookup2DTable"   type="TableFunctionType"/>
     <xsd:element name="Lookup1DTable"   type="TableFunctionType"/>
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
   </xsd:choice>
 </xsd:complexType> 




 
 <!--ELEMENT REGION-->
 <xsd:complexType name="ElementRegionType">
   <xsd:choice>
     <xsd:element name="LinearElasticOrthotropic" type="LinearElasticOrthotropicType"/>
     <xsd:element name="LinearElasticMaterial" type="LinearElasticMaterialType"/>
     <xsd:element name="DamageSphericalPoresMaterial" type="DamageSphericalPoresMaterialType"/>
     <xsd:element name="LinearElasticDEMMaterial" type="LinearElasticDEMMaterialType"/>
     <xsd:element name="DSIDDamageMaterial" type="DSIDDamageMaterialType"/>
   </xsd:choice>
   <xsd:attribute name="name" type="xsd:string"/>
   <xsd:attribute name="abaqusID" type="xsd:string"/>
   <xsd:attribute name="elementtype" type="xsd:string"/>
   <xsd:attribute name="hgDamp" type="xsd:double"/>
   <xsd:attribute name="hgStiff" type="xsd:double"/>
   <xsd:attribute name="materialName" type="xsd:string"/>
 </xsd:complexType>   

 <xsd:complexType name="ElementRegionsType">
  <xsd:sequence>
   <xsd:element name="ElementRegion" type="ElementRegionType" minOccurs="0" maxOccurs="unbounded"/>
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
  </xsd:sequence>
 </xsd:complexType>




 
 <!--PARTITION-->
 <xsd:complexType name="SpatialPartitionType">
   <xsd:attribute name="xpar" type="xsd:integer"/>
   <xsd:attribute name="ypar" type="xsd:integer"/>
   <xsd:attribute name="zpar" type="xsd:integer"/>
 </xsd:complexType>

 <xsd:complexType name="PartitionType">
  <xsd:choice>
   <xsd:element name="SpatialPartition" type="SpatialPartitionType" minOccurs="0" maxOccurs="1"/>
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
  </xsd:choice>
 </xsd:complexType>




 
 <!--OUTPUT-->
 <xsd:complexType name="OutputType">
   <xsd:attribute name="writePlot" type="xsd:int"/>
   <xsd:attribute name="writeRestart" type="xsd:int"/>
   <xsd:attribute name="restart_interval" type="unittype"/>
   <xsd:attribute name="plot_interval" type="unittype"/>
   <xsd:attribute name="plotIntervalTable" type="xsd:string"/>
   <xsd:attribute name="plotfile_root" type="xsd:string"/>
   <xsd:attribute name="slave_directory" type="xsd:string"/>
   <xsd:attribute name="parallel_silo" type="xsd:integer"/>
   <xsd:attribute name="writeFEMEdges" type="xsd:integer"/>
   <xsd:attribute name="writeFEMFaces" type="xsd:integer"/>
   <xsd:attribute name="fieldsToPlot" type="xsd:string"/>
   <xsd:attribute name="maxWallTime" type="xsd:integer"/>
 </xsd:complexType>




 
 <!-- JOINTS -->
 <xsd:complexType name="JointSetType">
   <!-- weight -->
   <xsd:attribute name="weight" type="xsd:double"/>
   <!-- random seed -->
   <xsd:attribute name="seed" type="xsd:unsignedInt"/>
   <xsd:attribute name="upVector" type="tensortype"/>
   <xsd:attribute name="northVector" type="tensortype"/>
   <!--strike length-->
   <xsd:attribute name="exponentStrikeDimension" type="unittype"/>
   <xsd:attribute name="minStrikeDimension" type="unittype"/>
   <xsd:attribute name="maxStrikeDimension" type="unittype"/>
   <!--dip length-->
   <xsd:attribute name="meanAspectRatio" type="xsd:double"/>
   <xsd:attribute name="stdevAspectRatio" type="xsd:double"/>
   <xsd:attribute name="minAspectRatio" type="xsd:double"/>
   <xsd:attribute name="maxAspectRatio" type="xsd:double"/>
   <!--strike angle-->
   <xsd:attribute name="meanStrikeAngle" type="unittype"/>
   <xsd:attribute name="stdevStrikeAngle" type="unittype"/>
   <xsd:attribute name="minStrikeAngle" type="unittype"/>
   <xsd:attribute name="maxStrikeAngle" type="unittype"/>
   <!--dip angle-->
   <xsd:attribute name="meanDipAngle" type="unittype"/>
   <xsd:attribute name="stdevDipAngle" type="unittype"/>
   <xsd:attribute name="minDipAngle" type="unittype"/>
   <xsd:attribute name="maxDipAngle" type="unittype"/>
   <!--hypocenter-->
   <xsd:attribute name="hurstExponent" type="xsd:double"/>
   <xsd:attribute name="meanFrequency" type="xsd:double"/>
   <xsd:attribute name="maxFrequency" type="xsd:double"/>
   <xsd:attribute name="stdevFrequency" type="xsd:double"/>
 </xsd:complexType>

 <xsd:complexType name="JointsType">
   <xsd:sequence>
    <xsd:element name="JointSet" type="JointSetType" minOccurs="0" maxOccurs="unbounded"/>
   </xsd:sequence>
   <xsd:attribute name="upVector" type="tensortype"/>
   <xsd:attribute name="northVector" type="tensortype"/>
   <xsd:attribute name="elementRegionName" type="xsd:string"/>
   <xsd:attribute name="nodeSetName" type="xsd:string"/>
   <xsd:attribute name="file" type="xsd:string"/>
 </xsd:complexType>


 <!-- Geometric objects -->
 <xsd:complexType name="GeometryListType">
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="Cylinder" type="CylinderGOType" />
      <xsd:element name="CylinderBy2Ends" type="CylinderGOType" />
      <xsd:element name="Sphere" type="SphereGOType" />
      <xsd:element name="Box" type="BoxGOType" />
      <xsd:element name="Ellipsoid" type="EllipsoidGOType" />
      <xsd:element name="Intersection" type="GeometryListType" />
      <xsd:element name="Union" type="GeometryListType" />
      <xsd:element name="Invert" type="GeometryListType" />
      <xsd:element name="And" type="GeometryListType" />
      <xsd:element name="Or" type="GeometryListType" />
      <xsd:element name="Not" type="GeometryListType"/>
      <xsd:element name="Transform" type="TransformGOType"/>
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
    </xsd:choice>
 </xsd:complexType>

 <xsd:complexType name="CylinderGOType">
   <xsd:attribute name="point1" type="tensortype"  use="required"/>
   <xsd:attribute name="point2" type="tensortype"  use="required"/>
   <xsd:attribute name="radius" type="unittype"  use="required"/>
 </xsd:complexType>


 <xsd:complexType name="TransformGOType">
    <xsd:complexContent> 
      <xsd:extension base="GeometryListType">  
        <xsd:attribute name="U" type="xsd:string"  use="required"/>
        <xsd:attribute name="r" type="tensortype"  use="required"/>
      </xsd:extension> 
    </xsd:complexContent> 
 </xsd:complexType>


 <xsd:complexType name="SphereGOType">
   <xsd:attribute name="center" type="tensortype"  use="required"/>
   <xsd:attribute name="radius" type="unittype"  use="required"/>
 </xsd:complexType>

 <xsd:complexType name="EllipsoidGOType">
   <xsd:attribute name="center" type="tensortype"  use="required"/>
   <xsd:attribute name="rx" type="unittype"  use="required"/>
   <xsd:attribute name="ry" type="unittype"  use="required"/>
   <xsd:attribute name="rz" type="unittype"  use="required"/>
 </xsd:complexType>

 <xsd:complexType name="BoxGOType">
   <xsd:attribute name="xmin" type="tensortype" use="required"/>
   <xsd:attribute name="xmax" type="tensortype" use="required"/>
 </xsd:complexType>



 
 <!-- NODESETS -->
 <xsd:complexType name="NodesetType">
   <xsd:sequence>
     <xsd:element name="Geometry" type="GeometryListType" minOccurs="0" maxOccurs="1"/>
   </xsd:sequence>
   <xsd:attribute name="name" type="xsd:string"/>
   <xsd:attribute name="type" type="xsd:int"/>
   <xsd:attribute name="xmin" type="tensortype"/>
   <xsd:attribute name="xmax" type="tensortype"/>
   <xsd:attribute name="point1" type="tensortype"/>
   <xsd:attribute name="point2" type="tensortype"/>
   <xsd:attribute name="radius" type="unittype"/>
 </xsd:complexType>

 <xsd:complexType name="NodesetsType">
   <xsd:choice minOccurs="0" maxOccurs="unbounded">
    <xsd:element name="Nodeset" type="NodesetType" />
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
   </xsd:choice>
 </xsd:complexType>




 
 <!--SOLVER APPLICATION-->
 <xsd:complexType name="ApplyType">
   <xsd:attribute name="solver" type="xsd:string"/>
   <xsd:attribute name="toregions" type="srangetype"/>
 </xsd:complexType>

 <xsd:complexType name="ApplicationType">
  <xsd:choice minOccurs="0" maxOccurs="unbounded">
   <xsd:element name="Apply" type="ApplyType" />
   <xsd:element name="Substep" type="SubstepType"/>
  </xsd:choice>
  <xsd:attribute name="name" type="xsd:string"/>
  <xsd:attribute name="begintime" type="unittype"/>
  <xsd:attribute name="endtime" type="unittype"/>
  <xsd:attribute name="dt" type="unittype"/>
 </xsd:complexType>

 <xsd:complexType name="SolverApplicationType">
  <xsd:sequence>
   <xsd:element name="Application" type="ApplicationType" minOccurs="0" maxOccurs="unbounded"/>
    <!-- <xsd:element name="Include" type="IncludeType" /> -->
     <xsd:group ref="IfIncludeOption"/>
  </xsd:sequence>
 </xsd:complexType>




 
 <!-- PARENT -->
 <xsd:element name="Problem" type="ProblemType"/>
 
 <xsd:complexType name="ProblemType">
  <xsd:all>
   <xsd:element name="Units" type="UnitsType" minOccurs="0" maxOccurs="1"/>
   <!-- <xsd:element name="Materials" type="MaterialsType" minOccurs="0" maxOccurs="1"/> -->
   <xsd:element name="Fracture" type="FractureType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="HACK" type="HackType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Mesh" type="MeshType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="JointSets" type="JointsType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Nodesets" type="NodesetsType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Solvers" type="SolversType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="SolverApplications" type="SolverApplicationType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="ElementRegions" type="ElementRegionsType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Contact" type="ContactType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="EllipsoidalDEM" type="EllipsoidalDEMType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="InitialConditions" type="InitialConditionsType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="BoundaryConditions" type="BoundaryConditionsType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="InitialConstitutiveValues" type="InitialConstitutiveValuesType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="EnforcedConstitutiveValues" type="EnforcedConstitutiveValuesType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Tables" type="TablesType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Functions" type="FunctionsType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Partition" type="PartitionType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Output" type="OutputType" minOccurs="0" maxOccurs="1"/>
   <xsd:element name="Parameters" type="ParametersType" minOccurs="0" />
   <xsd:element name="If" type="IFType" minOccurs="0" />
   <xsd:element name="Include" type="IncludeType" minOccurs="0" />
  </xsd:all>
